@using OzelDersler.Web.Areas.Teachers.Models;
@model List<StudentCourseDto>
@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="text-center">
        <h1 class="display-4 mb-4">Ders Almak İsteyen Öğrenciler</h1>
    </div>
</div>
<div class="mb-4">
    @await Component.InvokeAsync("Branches")
</div>
<div class="row">
    <div class="col-12">
        <div class="row">
            @foreach (var course in Model)
            {
                <div class="col-md-3">
                    @await Html.PartialAsync("_CoursePartial", course)
                </div>
            }
        </div>
    </div>
</div>
