@model TeacherCourseDto
    <div class="card mb-3" style="width: 18rem;">
        <img class="card-img-top" src="~/images/@Model.FirstName" height="300" width="100" alt="teacher-resim">
        <div class="card-body">
            <h5 class="card-title">@Model.FirstName @Model.LastName</h5>
        </div>
        <ul class="list-group list-group-flush">
            <li class="list-group-item">@Model.Experience Yıllık Deneyim</li>
        </ul>
        <div class="card-body">
            <a asp-controller="Home" asp-action="Detail" class="card-link text-decoration-none">İlana Git</a>
        </div>
    </div>
