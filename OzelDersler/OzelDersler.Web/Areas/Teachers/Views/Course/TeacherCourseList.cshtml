@model List<TeacherCourseDto>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4 mb-4">Öğretmenler</h1>
</div>
<div class="row">
    <div class="col-12">
        <div class="row">
            @foreach (var course in Model)
            {
                <div class="col-md-4">
                    @await Html.PartialAsync("_TeacherCoursePartial", course)
                </div>
            }
        </div>
    </div>
</div>
